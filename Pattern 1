Program 3
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;
    printf("enter oa number");
    scanf("%d",&n);
    // Write C code here
    for(int i=1;i<=n;i++)
    {
        for(int j=i;j>=1;j--)
        {
            printf("%d",i);
        }
        printf("\n");
    }
    return 0;
}

