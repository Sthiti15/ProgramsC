Program 3:
#include <stdio.h>

int main() {
    // Write C code here
    int n=5;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            printf("%2d" ,i*j);
        }
        printf("\n");
    }
    return 0;
}

