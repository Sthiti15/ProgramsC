Program 2:
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n=5,k=1;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            printf("%2d" ,k);
            k+=2;
        }
        printf("\n");
    }
    return 0;
}

